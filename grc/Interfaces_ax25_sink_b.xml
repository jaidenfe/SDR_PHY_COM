<?xml version="1.0"?>
<block>
  <name>AX.25 Sink</name>
  <key>Interfaces_ax25_sink_b</key>
  <category>[Interfaces]/AX25</category>
  <import>import Interfaces</import>
  <make>Interfaces.ax25_sink_b($src, $dst, $log)</make>

  <param>
    <name>Source</name>
    <key>src</key>
    <type>string</type>
  </param>
  <param>
    <name>Destination</name>
    <key>dst</key>
    <type>string</type>
  </param>

  <param>
    <name>Log</name>
    <key>log</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
    <vlen>$&gt;)</vlen>
  </sink>
</block>
