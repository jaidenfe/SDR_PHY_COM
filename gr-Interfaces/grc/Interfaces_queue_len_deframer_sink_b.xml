<?xml version="1.0"?>
<block>
  <name>Queue Length-Based Framer Sink</name>
  <key>Interfaces_queue_len_deframer_sink_b</key>
  <category>Interfaces</category>
  <import>import Interfaces</import>
  <make>Interfaces.queue_len_deframer_sink_b('U', $rxlog)</make>

  <param>
    <name>Log</name>
    <key>rxlog</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
    <vlen>$&gt;)</vlen>
  </sink>
</block>
